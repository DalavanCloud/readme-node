  "repository": {
    "type": "git",
    "url": "git@github.com:readme/metrics.git"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "express": "^4.16.3",
    "jsinspect": "^0.12.7",
    "mocha": "^5.0.0",
    "nock": "^9.2.6",
    "nyc": "^11.7.1",
    "prettier": "^1.12.1",
    "supertest": "^3.1.0"
  }
