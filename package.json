{
  "name": "@readme/metrics",
  "description": "Track your API metrics within ReadMe",
  "version": "0.0.0",
  "dependencies": {
    "config": "file:config",
    "configly": "^4.1.0",
    "r2": "^2.0.1"
  },
  "scripts": {
    "lint": "eslint -f unix .",
    "inspect": "jsinspect",
    "prettier": "prettier --list-different \"./**/**.js\"",
